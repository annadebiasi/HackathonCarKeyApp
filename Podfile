  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'CocoaMQTT'

  # Pods for testWatchApp

  target 'testWatchAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'testWatchAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

